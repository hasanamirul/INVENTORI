{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    {
      "src": "/(css|js|images|favicon.ico|robots.txt)(.*)",
      "dest": "/public/$1$2"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_URL": "https://nama-proyek-anda.vercel.app",
    "LOG_CHANNEL": "stderr",
    "VIEW_COMPILED_PATH": "/tmp",
    "SESSION_DRIVER": "cookie"
  }
}